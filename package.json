{
  "name": "pulse-js-framework",
  "version": "1.5.0",
  "description": "A declarative DOM framework with CSS selector-based structure and reactive pulsations",
  "type": "module",
  "main": "index.js",
  "types": "types/index.d.ts",
  "bin": {
    "pulse": "cli/index.js"
  },
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "default": "./index.js"
    },
    "./runtime": {
      "types": "./types/index.d.ts",
      "default": "./runtime/index.js"
    },
    "./runtime/pulse": {
      "types": "./types/pulse.d.ts",
      "default": "./runtime/pulse.js"
    },
    "./runtime/dom": {
      "types": "./types/dom.d.ts",
      "default": "./runtime/dom.js"
    },
    "./runtime/router": {
      "types": "./types/router.d.ts",
      "default": "./runtime/router.js"
    },
    "./runtime/store": {
      "types": "./types/store.d.ts",
      "default": "./runtime/store.js"
    },
    "./runtime/native": {
      "types": "./types/index.d.ts",
      "default": "./runtime/native.js"
    },
    "./runtime/logger": {
      "types": "./types/logger.d.ts",
      "default": "./runtime/logger.js"
    },
    "./runtime/hmr": {
      "types": "./types/hmr.d.ts",
      "default": "./runtime/hmr.js"
    },
    "./runtime/lru-cache": "./runtime/lru-cache.js",
    "./runtime/utils": "./runtime/utils.js",
    "./compiler": {
      "types": "./types/index.d.ts",
      "default": "./compiler/index.js"
    },
    "./compiler/lexer": "./compiler/lexer.js",
    "./compiler/parser": "./compiler/parser.js",
    "./compiler/transformer": "./compiler/transformer.js",
    "./core/errors": "./core/errors.js",
    "./vite": {
      "types": "./types/index.d.ts",
      "default": "./loader/vite-plugin.js"
    },
    "./mobile": "./mobile/bridge/pulse-native.js",
    "./package.json": "./package.json"
  },
  "files": [
    "index.js",
    "cli/",
    "core/",
    "runtime/",
    "compiler/",
    "loader/",
    "mobile/",
    "types/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "test": "npm run test:compiler && npm run test:sourcemap && npm run test:pulse && npm run test:dom && npm run test:router && npm run test:store && npm run test:hmr && npm run test:lint && npm run test:format && npm run test:analyze",
    "test:compiler": "node test/compiler.test.js",
    "test:sourcemap": "node test/sourcemap.test.js",
    "test:pulse": "node test/pulse.test.js",
    "test:dom": "node test/dom.test.js",
    "test:router": "node test/router.test.js",
    "test:store": "node test/store.test.js",
    "test:hmr": "node test/hmr.test.js",
    "test:lint": "node test/lint.test.js",
    "test:format": "node test/format.test.js",
    "test:analyze": "node test/analyze.test.js",
    "build:netlify": "node scripts/build-netlify.js",
    "version": "node scripts/sync-version.js",
    "docs": "node cli/index.js dev docs"
  },
  "keywords": [
    "framework",
    "frontend",
    "reactive",
    "declarative",
    "dom",
    "pulse",
    "css-selectors",
    "dsl",
    "ui",
    "javascript",
    "spa",
    "signals"
  ],
  "author": "Vincent Hirtz <hirtzvincent@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vincenthirtz/pulse-js-framework.git"
  },
  "homepage": "https://github.com/vincenthirtz/pulse-js-framework#readme",
  "bugs": {
    "url": "https://github.com/vincenthirtz/pulse-js-framework/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {},
  "devDependencies": {}
}
