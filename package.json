{
  "name": "pulse-js-framework",
  "version": "1.7.38",
  "description": "A declarative DOM framework with CSS selector-based structure and reactive pulsations",
  "type": "module",
  "sideEffects": false,
  "main": "index.js",
  "types": "types/index.d.ts",
  "bin": {
    "pulse": "cli/index.js"
  },
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "default": "./index.js"
    },
    "./runtime": {
      "types": "./types/index.d.ts",
      "default": "./runtime/index.js"
    },
    "./runtime/lite": {
      "types": "./types/lite.d.ts",
      "default": "./runtime/lite.js"
    },
    "./runtime/pulse": {
      "types": "./types/pulse.d.ts",
      "default": "./runtime/pulse.js"
    },
    "./runtime/dom": {
      "types": "./types/dom.d.ts",
      "default": "./runtime/dom.js"
    },
    "./runtime/router": {
      "types": "./types/router.d.ts",
      "default": "./runtime/router.js"
    },
    "./runtime/store": {
      "types": "./types/store.d.ts",
      "default": "./runtime/store.js"
    },
    "./runtime/native": {
      "types": "./types/index.d.ts",
      "default": "./runtime/native.js"
    },
    "./runtime/logger": {
      "types": "./types/logger.d.ts",
      "production": "./runtime/logger.prod.js",
      "development": "./runtime/logger.js",
      "default": "./runtime/logger.js"
    },
    "./runtime/logger/prod": "./runtime/logger.prod.js",
    "./runtime/logger/dev": "./runtime/logger.js",
    "./runtime/hmr": {
      "types": "./types/hmr.d.ts",
      "default": "./runtime/hmr.js"
    },
    "./runtime/lru-cache": "./runtime/lru-cache.js",
    "./runtime/utils": "./runtime/utils.js",
    "./runtime/dom-adapter": "./runtime/dom-adapter.js",
    "./runtime/async": {
      "types": "./types/async.d.ts",
      "default": "./runtime/async.js"
    },
    "./runtime/form": {
      "types": "./types/form.d.ts",
      "default": "./runtime/form.js"
    },
    "./runtime/http": {
      "types": "./types/http.d.ts",
      "default": "./runtime/http.js"
    },
    "./runtime/a11y": {
      "types": "./types/a11y.d.ts",
      "default": "./runtime/a11y.js"
