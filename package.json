{
  "name": "pulse-js-framework",
  "version": "1.7.13",
  "description": "A declarative DOM framework with CSS selector-based structure and reactive pulsations",
  "type": "module",
  "main": "index.js",
  "types": "types/index.d.ts",
  "bin": {
    "pulse": "cli/index.js"
  },
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "default": "./index.js"
    },
    "./runtime": {
      "types": "./types/index.d.ts",
      "default": "./runtime/index.js"
    },
