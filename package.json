    },
    "./runtime/form": {
      "types": "./types/form.d.ts",
      "default": "./runtime/form.js"
    },
    "./runtime/http": {
      "types": "./types/http.d.ts",
      "default": "./runtime/http.js"
    },
    "./runtime/a11y": {
      "types": "./types/a11y.d.ts",
      "default": "./runtime/a11y.js"
    },
    "./runtime/devtools": "./runtime/devtools.js",
    "./runtime/websocket": {
      "types": "./types/websocket.d.ts",
      "default": "./runtime/websocket.js"
    },
    "./runtime/graphql": {
      "types": "./types/graphql.d.ts",
      "default": "./runtime/graphql.js"
    },
    "./compiler": {
      "types": "./types/index.d.ts",
      "default": "./compiler/index.js"
    },
    "./compiler/lexer": "./compiler/lexer.js",
    "./compiler/parser": "./compiler/parser.js",
    "./compiler/transformer": "./compiler/transformer.js",
    "./core/errors": "./runtime/errors.js",
    "./vite": {
      "types": "./types/index.d.ts",
      "default": "./loader/vite-plugin.js"
    },
    "./mobile": "./mobile/bridge/pulse-native.js",
    "./package.json": "./package.json"
  },
  "files": [
    "index.js",
    "cli/",
    "core/",
    "runtime/",
    "compiler/",
    "loader/",
